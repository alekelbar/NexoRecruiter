@page "/dashboard";
@using Microsoft.AspNetCore.Identity
@using NexoRecruiter.Web.Features.Auth.Models
@using NexoRecruiter.Web.Features.Dashboard.Helpers

@attribute [Authorize]
@layout DashboardLayout

<MudContainer MaxWidth="MaxWidth.ExtraExtraLarge">
    <MudText Typo="Typo.h5">Welcome Admin</MudText>
</MudContainer>

@code {
    [CascadingParameter(Name = "CurrentUser")]
    public ApplicationUser? CurrentUser { get; set; }
    [CascadingParameter(Name = "CurrentRoles")]
    public string? CurrentRoles { get; set; }
}
